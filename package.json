{
  "name": "vscode-vega-viewer",
  "displayName": "Vega Viewer",
  "description": "VSCode extension for Interactive Preview of Vega & Vega-Lite maps ðŸ—ºï¸ & graphs ðŸ“ˆ ",
  "version": "8.4.0",
  "icon": "images/vega-viewer.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak",
  "contributors": [
    "Taras Novak"
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/vscode-vega-viewer"
  },
  "bugs": "https://github.com/RandomFractals/vscode-vega-viewer/issues",
  "homepage": "https://github.com/RandomFractals/vscode-vega-viewer/README.md",
  "keywords": [
    "vega",
    "vega-lite",
    "viewer",
    "preview",
    "charts ðŸ“Š",
    "maps ðŸ—ºï¸",
    "graphs ðŸ“ˆ",
    "dataViz",
    "vegaViz",
    "dev tools ðŸ”¬",
    "visual vocabulary ðŸ“˜",
    "chart examples",
    "interactive preview",
    "svg chart",
    "png chart",
    "gist vega chart"
  ],
  "engines": {
    "vscode": "^1.36.0",
    "node": ">=10.2.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:vega.create",
    "onCommand:vega.preview",
    "onCommand:vega.preview.remote",
    "onCommand:vega.visual.vocabulary",
    "onCommand:vega.examples",
    "onCommand:vega.examples.lite",
    "onWebviewPanel:vega.preview",
    "onWebviewPanel:vega.visual.vocabulary",
    "onLanguage:json"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "vega.create",
        "title": "Create Vega Spec",
        "category": "Vega"
      },
      {
        "command": "vega.preview",
        "title": "Preview Vega Graph",
        "category": "Vega",
        "icon": {
          "light": "./images/vega-viewer.svg",
          "dark": "./images/vega-viewer.svg"
        }
      },
      {
        "command": "vega.preview.remote",
        "title": "Preview Remote Vega Graph",
        "category": "Vega"
      },
      {
        "command": "vega.visual.vocabulary",
        "title": "Visual Vocabulary Examples",
        "category": "Vega"
      },
      {
        "command": "vega.examples",
        "title": "Examples",
        "category": "Vega"
      },
      {
        "command": "vega.examples.lite",
        "title": "Lite Examples",
        "category": "Vega"
      }
    ],
    "keybindings": [
      {
        "command": "vega.create",
        "key": "ctrl+alt+c"
      },
      {
        "command": "vega.preview",
        "key": "ctrl+alt+v"
      },
      {
        "command": "vega.preview.remote",
        "key": "ctrl+alt+r"
      },
      {
        "command": "vega.visual.vocabulary",
        "key": "ctrl+alt+q"
      },
      {
        "command": "vega.examples",
        "key": "ctrl+alt+x"
      },
      {
        "command": "vega.examples.lite",
        "key": "ctrl+alt+l"
      }
    ],
    "languages": [
      {
        "id": "json",
        "aliases": [
          "JSON",
          "Vega",
          "vg",
          "vl"
        ],
        "extensions": [
          ".vega",
          ".vg",
          ".vg.json",
          ".vl",
          ".vl.json"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "*.vega",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vg",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vg.json",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vl",
        "url": "https://vega.github.io/schema/vega-lite/v4.json"
      },
      {
        "fileMatch": "*.vl.json",
        "url": "https://vega.github.io/schema/vega-lite/v4.json"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vega Viewer",
      "properties": {
        "vega.viewer.dataPreviewCommand": {
          "type": "string",
          "enum": [
            "vscode.open",
            "data.preview"
          ],
          "default": "vscode.open",
          "description": "Vega spec CSV & JSON data files preview command"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "json-stringify-pretty-compact": "^2.0.0",
    "lz-string": "^1.4.4",
    "vega": "^5.10.0",
    "vega-lite": "^4.6.0",
    "vega-embed": "^6.4.0"
  }
}

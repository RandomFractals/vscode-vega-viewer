{
  "name": "vscode-vega-viewer",
  "displayName": "Vega Viewer",
  "description": "Vega Viewer",
  "version": "4.4.1",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak",
  "contributors": [
    "Taras Novak"
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/vscode-vega-viewer"
  },
  "bugs": "https://github.com/RandomFractals/vscode-vega-viewer/issues",
  "homepage": "https://github.com/RandomFractals/vscode-vega-viewer/README.md",
  "keywords": [
    "vega",
    "vega-lite",
    "viewer",
    "preview",
    "charts",
    "maps",
    "graphs",
    "dataViz",
    "vegaViz",
    "devTools"
  ],
  "engines": {
    "vscode": "^1.32.3"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:vega.create",
    "onCommand:vega.preview",
    "onCommand:vega.examples",
    "onCommand:vega.examples.lite",
    "onWebviewPanel:vega.preview",
    "onLanguage:json"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "vega.create",
        "title": "Create Vega ðŸ“Š",
        "category": "Vega"
      },
      {
        "command": "vega.preview",
        "title": "Preview ðŸ“Š",
        "category": "Vega"
      },
      {
        "command": "vega.examples",
        "title": "Examples ðŸ“Š",
        "category": "Vega"
      },
      {
        "command": "vega.examples.lite",
        "title": "Lite Examples ðŸ“Š",
        "category": "Vega"
      }
    ],
    "keybindings": [
      {
        "command": "vega.create",
        "key": "ctrl+alt+c"
      },
      {
        "command": "vega.preview",
        "key": "ctrl+alt+v"
      },
      {
        "command": "vega.examples",
        "key": "ctrl+alt+x"
      },
      {
        "command": "vega.examples.lite",
        "key": "ctrl+alt+l"
      }
    ],
    "languages": [
      {
        "id": "json",
        "aliases": [
          "Vega",
          "vg",
          "vl"
        ],
        "extensions": [
          ".vega",
          ".vg",
          ".vg.json",
          ".vl",
          ".vl.json"
        ]
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "*.vega",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vg",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vg.json",
        "url": "https://vega.github.io/schema/vega/v5.json"
      },
      {
        "fileMatch": "*.vl",
        "url": "https://vega.github.io/schema/vega-lite/v3.json"
      },
      {
        "fileMatch": "*.vl.json",
        "url": "https://vega.github.io/schema/vega-lite/v3.json"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vega|vg.json|vl.json|vg|vl)/",
          "command": "vega.preview",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.2",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "vega": "^5.3.5",
    "vega-lite": "^3.2.0",
    "vega-embed": "^4.0.0"
  }
}
